<?xml version="1.0" encoding="UTF-8"?>
<project name="logging" >

    <macrodef name="log.info">
        <attribute name="task" />
        <attribute name="message" />
        <sequential>
            <echo message="[@{task}] @{message}" />
        </sequential>
    </macrodef>


