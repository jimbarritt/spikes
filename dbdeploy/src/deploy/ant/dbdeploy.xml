<?xml version="1.0" encoding="UTF-8"?>
<project name="dbdeploy" >

    <import file="../../build/ant/macros/logging.xml" />
    
    <macrodef name="dbdeploy.init-live">
        <attribute name="src.dbdeploy.dir" />
        <sequential>
            <log.info task="dbdeploy.init-live" message="Initialising the database in live mode from [@{src.dbdeploy.dir}]" />            
        </sequential>
    </macrodef>

</project>